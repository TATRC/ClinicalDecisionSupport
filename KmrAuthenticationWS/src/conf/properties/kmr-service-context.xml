      <entry key="serviceEndpoint" value="http://kmrdev.medsphere.com:8080/MSCServiceApp/MSCAuthenticationService?wsdl"/>
      <entry key="inboxURL" value="http://208.75.163.61:8080/UniversalInboxGUI/inbox.iface?"/>
   </util:map>

</beans>
